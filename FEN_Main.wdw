     code : |1-
      PROCEDURE MaFenêtre()
      
      //CONSTANT MY_STATOSHI_STACK     = "MY_STATOSHI_STACK"
      //CONSTANT MONTRE_JAIDESBITCOINS = "INT_JAIDESBITCOINS"
      //CONSTANT ALARM_PRICE			 = "ALARM_PRICE"
      //CONSTANT NB_SATOSHI_PAR_BTC =  100 000 000
      //gnMySatStack est un entier sur 8 = ChargeParamètre(MY_STATOSHI_STACK)
      //prixCourant				est un monétaire
      //dateDernierPrixCourant	est une dateheure
      //// prix auquel lancer une alarme
      //gPrixAlarme 			est monétaire //= 1000
      
      local
      	//Init prix alarme
      	nPrixAlarme est entier = ChargeParamètre(ALARM_PRICE,0)
      	gPrixAlarme = nPrixAlarme
      
      
      
      si gnMySatStack = 0 alors
      	GR_JAIDESBITCOINS..Visible = faux
      sinon
      	LIB_NbBitcoin = gnMySatStack
      	INT_JAIDESBITCOINS = ChargeParamètre(MONTRE_JAIDESBITCOINS, 0)
      fin
      GR_SAISIE_BTC..Visible = Faux
      SI INT_JAIDESBITCOINS = 0 ALORS
      	// ne pas montrer le solde en btc
      	GR_JAIDESBITCOINS..Visible = Faux
      FIN
      
      // récup dernier prix connu
      (prixCourant,dateDernierPrixCourant) = get_last_priceAndDate(gsDevise)
      // MAJ des libellé liés au prix
      MAJ_LIbelle()
      
      // Timer de MAJ : toutes les 2 heures
      TimerSys(Callback_timer,100*60*2,1)                                         
      	procédure interne Callback_timer()
      		refresh_price()
      	fin
