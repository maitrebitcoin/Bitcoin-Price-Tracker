  procedures :
   -
     name : refresh_price
     procedure_id : 2249889678534841022
     type_code : 14
     code : |1+
      // demander a récupérer le prix a nouveau
      procédure refresh_price()
      
      
      // appel d'un webservice pour avoir le prix
      // par défait en Euros
      sPair est une chaîne = "BTCEUR"
      sNomJson est chaine  = "XXBTZEUR"
      // si on veut le prix en $ américains
      si gsDevise = "USD" ALORS
      	sPair     = "BTCUSD"
      	sNomJson  = "XXBTZUSD"
      FIN
      
      
      // appel WS  pour récu"rer le dernier prix de marché 
      getPrixFromKraken_Async(sPair, sNomJson, _onRécuperPrix)
      procédure interne _onRécuperPrix( dernierprix est un monétaire  )
      	// MAJ globale
      	prixCourant = dernierprix
      	// ajout a la base
      	add_price(prixCourant,gsDevise)
      	// pour forcer l'animation
      	LIB_Prix_1_BTC = 0
      	// MAJ Libellé liés au prix
      	MAJ_LIbelle()
      fin
      
     type : 458752
   -
     name : MAJ_LIbelle
     procedure_id : 2252399163494389017
     type_code : 14
     code : |1+
      procédure MAJ_LIbelle()
      
      // blindage
      si prixCourant=0 ALORS
      	retour
      FIN
      
      
      // MAJ masque
      SELON gsDevise
      	CAS "EUR"
      		LIB_Prix_1_BTC..MasqueSaisie = "999 999 €"
      		LIB_Total_BTC..MasqueSaisie =  "999 999 999 €"
      	CAS "USD"
      		LIB_Prix_1_BTC..MasqueSaisie = "$$ 999 999"
      		LIB_Total_BTC..MasqueSaisie  = "$$ 999 999 999"
      	AUTRE CAS
      		dbgAssertion(faux, gsDevise)
      FIN
      // MAJ libellé
      LIB_Prix_1_BTC = prixCourant
      // Total si on a des BTC
      LIB_Total_BTC = prixCourant * gnMySatStack / NB_SATOSHI_PAR_BTC
      	
      	
      	
      // ath ?
      ath est un monétaire
      ath = get_ath(gsDevise)
      // si on est en ath
      si prixCourant>=ath ALORS
      	IMG_ALLTIMEHIGH_TOTAL..Visible = vrai
      sinon	
      	IMG_ALLTIMEHIGH_TOTAL..Visible = faux
      FIN
      
      // calcul évolution 24 h
      prixRef_jour est un monétaire
      prixRef_jour = get_price_start_day(gsDevise)
       prixRef_semaine est un monétaire
      prixRef_semaine = get_price_start_week(gsDevise)
      prixRef_annee  est un monétaire
      prixRef_annee = get_price_start_year(gsDevise)
      
      _MaEvolutionPeriode(prixCourant,prixRef_jour,    LIB_Evolution_Jour   )
      _MaEvolutionPeriode(prixCourant,prixRef_semaine, LIB_Evolution_Semaine)
      _MaEvolutionPeriode(prixCourant,prixRef_annee,   LIB_Evolution_Année)
      
      // MAJ contre valeur si on a btc
      _MAJTotalFiat()
      
      // gestion des alarmes
      si gPrixAlarme<>0 _et_ prixCourant>=gPrixAlarme ALORS
      	// alarme !
      	notif est une Notification
      	//notif.Titre	= "Le prix du Bitcoin a atteinds " + gPrixAlarme
      	notif.Message	  = <§$0000§> + rc + LIB_Prix_1_BTC..ValeurAffichée + " !!"
      	<compile si TypeConfiguration=Android>
      		notif.Titre       =  "1 BTC = " + LIB_Prix_1_BTC..ValeurAffichée 
      		notif.GrandeIcone = "coupe.png" //"bitcoin_orange_64@dpi1x.png" 	
      	<fin>
      	notif.ActionClic = "procClicNotif"
      	NotifAjoute(notif)
      	//pour ne pas notifier 2x
      	gPrixAlarme = 0 
      FIN
     type : 458752
   -
     name : getPrixFromKraken_Async
     procedure_id : 2261272316812339380
     type_code : 14
     code : |1+
      procédure getPrixFromKraken_Async( LOCAL sPair chaîne  ANSI,local sNomJson est chaine ANSI, callback procédure  )
      
      _callback est une procédure = callback
      TâcheParallèleExécute(_tache,( ))
      procédure interne _tache()
      	// récupération du prix via un webservice - bloquant 
      	prix est un monétaire = _dGetPrixFromKraken(sPair, sNomJson )
      	si prix =0 ALORS
      		// echec
      		retour
      	FIN
      	// Appel callback dans la thread principal
      	ExécuteThreadPrincipal(_callback, prix)
      	
      fin
     type : 458752
   -
     name : _dGetPrixFromKraken
     procedure_id : 2249892186795866821
     type_code : 14
     code : |1-
      PROCEDURE _dGetPrixFromKraken(local sPair chaîne  ANSI,local sNomJson est chaine ANSI ) : monétaire
      
      QUAND EXCEPTION dans
      
      
      sUrl est chaîne  ANSI = "https://api.kraken.com/0/public/Trades?pair=" + sPair 
      
      sNomRech est chaîne = sNomJson// ex : "XETHZEUR"
      
      //@A voir
      //// lancer le requete,  version asynchrone
      //requeteGetTrade est un httpRequête
      //requeteGetTrade.URL = sUrl
      //p est une procédure =_callbackReponse
      //requeteGetTrade.Envoie(  p )
      //procédure interne _callbackReponse( laRéponse est un httpRéponse  )
      //	
      //fin
      
      // demande les dernières transaction a KRAKEN
      bRes est un booléen = HTTPRequête(sUrl)
      SI PAS bRes ALORS
      	RENVOYER  0
      FIN
      
      
      // récup du résultat de HTTPRequête()
      sJSON est chaîne ANSI 
      sJSON = HTTPDonneRésultat(httpRésultat)
      
      //Trace(s)
      vJsWW est un variant =JSONVersVariant(sJSON)
      vJs est variant = vJsWW.result //.XETHEUR //[1000]
      
      // récup date
      slast est chaine =  vJs.last
      si slast<"1600502946233061985" ALORS
      	// vieux résultat, on ignore
      	renvoyer 0
      FIN
      
      
      //trace( vjsWW.result.XETHZEUR[1000])
      
      //soit bVjs_Tab = vjs= vjs.XETHEUR //[999] //[1000]
      POUR TOUT _Membre DE vJs..Membre 
      	//soit nNbItem = _Membre..Membre..Occurrence 
      	Vjs_Tab est variant =  _Membre..Valeur
      	//SI  _Membre..Nom = "XETHZEUR" ALORS
      	SI  _Membre..Nom = sNomRech ALORS
      		LastItem est variant = Vjs_Tab[Vjs_Tab..Occurrence]
      		LastPrice est variant =  LastItem[1] 
      		dPrix est un réel = LastPrice
      		// bliondage valeuer >1000 000 000 lue le 20/10/2017
      		si (dPrix > 100000000) ALORS
      			renvoyer 0
      		FIN
      		dbgAssertion(dPrix>1000)
      		RENVOYER dPrix
      	FIN
      FIN
      
      faire
      	renvoyer 0
      fin
      
      RENVOYER 0
     type : 458752
   -
     name : _MaEvolutionPeriode
     procedure_id : 2251245973362866515
     type_code : 14
     code : |1+
      // maj d'un libellé d’évolution du prix.
      procédure _MaEvolutionPeriode(_prixCourant monétaire, prixRef est un monétaire, champDest est champ)
      
      // calcul valeur a afficher
      SI prixRef = 0 ALORS
      	champDest = "?"
      	RETOUR
      FIN
      evolution			est un réel	= (_prixCourant - prixRef) / prixRef
      evolution_formaté	est chaine	
      si evolution=0 alors
      	evolution_formaté = "="
      sinon si evolution >0 ALORS
      	evolution_formaté	= "+ " + NumériqueVersChaîne(evolution*100, "4,2")+ " %"
      sinon
      	evolution_formaté	= "- " + NumériqueVersChaîne(-evolution*100, "4,2")+ " %"
      FIN
      
      
      // Maj champ
      champDest			=  evolution_formaté
      SI evolution>=0 ALORS
      	champDest ..Couleur	= VertFoncé
      SINON
      	champDest ..Couleur	= RougeFoncé
      FIN
